#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/felipe/coding/contra-cheque-prod/node_modules/.pnpm/vitest@3.1.4_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_tsx@4.19.4/node_modules/vitest/node_modules:/Users/felipe/coding/contra-cheque-prod/node_modules/.pnpm/vitest@3.1.4_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_tsx@4.19.4/node_modules:/Users/felipe/coding/contra-cheque-prod/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/felipe/coding/contra-cheque-prod/node_modules/.pnpm/vitest@3.1.4_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_tsx@4.19.4/node_modules/vitest/node_modules:/Users/felipe/coding/contra-cheque-prod/node_modules/.pnpm/vitest@3.1.4_@types+node@22.15.21_jiti@2.4.2_lightningcss@1.30.1_tsx@4.19.4/node_modules:/Users/felipe/coding/contra-cheque-prod/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
